<h1>Users listing page</h1>
<% @user.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.public_key %></td>
    <td><%= link_to 'Show', user_path(user) %></td>
    <% if current_user.admin? && current_user != user %>
      <td><%= link_to 'Delete', destroy_path(user), method: :delete %></td>
    <% end %>
    </br>
  </tr>
<% end %>















